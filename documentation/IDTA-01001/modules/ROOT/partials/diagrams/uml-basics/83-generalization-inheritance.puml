@startuml
!theme idta from https://raw.githubusercontent.com/admin-shell-io/aas-specs-antora/main/plantuml/

skinparam packageStyle rectangle


package "A)" {
    class Class1 {
      +class2: Class2
    }
    class Class3
    Class1<|--Class3
}

package "B)" {
    class Class3<Class1>
}

package "C)" {
    class Class3<Class1> {
      .. ::Class1 ..
      +class2: Class2
    }
}

@enduml