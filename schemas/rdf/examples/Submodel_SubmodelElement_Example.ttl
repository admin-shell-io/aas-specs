        aas:Category "CONSTANT"^^xsd:string ;

        aas:hasKind aas:INSTANCE ;

        #--------------------------------
        # 1) Reference Element with Keys
        aas:semanticId [
            a aas:Reference ;
            aas:key [
                a aas:Key ;
                aas:Key/type aas:KeyElements/GLOBAL_REFERENCE ;
                aas:Key/value "0173-1#02-AAO677#002"^^xsd:string ;
                aas:Key/idType aas:KeyType/IRDI ;
